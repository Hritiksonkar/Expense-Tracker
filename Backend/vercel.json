{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node@3.0.0"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/health",
            "dest": "/index.js"
        },
        {
            "src": "/",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "index.js": {
            "runtime": "nodejs18.x",
            "maxDuration": 30
        }
    }
}